{
	"console": {
		"log_level": 1
	},

	"logs": {
		"enabled": false,
		"log_dir": "logs",
		"timestamp": true,
		"log_events": {
			"http_event": true,
			"sever_event": true
		}
	},

	"router": {
		"config_file": ".httpconf",
		"http_docs": "www",
		"server_docs": "docs",
		"index_file": [
			"index.html",
			"index.htm",
			"default.html",
			"default.htm"
		]
	},

	"socket": {
		"host": "0.0.0.0",
		"port": 3698,
		"timeout": 30,
		"max_clients": 1000,
		"recv_size": 1024
	}
}